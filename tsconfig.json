{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "outDir": "dist",
    "noEmit": true,
    "paths": {
      "#3p/*": ["./3p/*"],
      "#ads/*": ["./ads/*"],
      "#build/*": ["./build/*"],

      "#builtins/*": ["./src/builtins/*"],
      "#compiler/*": ["./src/compiler/*"],
      "#core/*": ["./src/core/*"],
      "#experiments/*": ["./src/experiments/*"],
      "#inabox/*": ["./src/inabox/*"],
      "#polyfills/*": ["./src/polyfills/*"],
      "#preact/*": ["./src/preact/*"],
      "#purifier/*": ["./src/purifier/*"],
      "#service/*": ["./src/service/*"],

      "#test/*": ["./test/*"],
      "#testing/*": ["./testing/*"],

      "#third_party/*": ["./third_party/*"]
    }
  },
  "include": [
    "src",
    "3p",
    "ads",
    "extensions",
    "test",
    "testing",
    "third_party",
  ],
  "exclude": [
    "build",
    "dist",
    "dist.*",
    "node_modules",
  ]
}
